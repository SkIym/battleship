{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base",
        "prettier"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "ignorePatterns": ["*.test.js"],
    "rules": {
        "consistent-return": "off",
        "no-param-reassign": "off"
    }
}
